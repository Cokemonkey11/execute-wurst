package Units2
import UnitObjEditing

public constant ID_TC = 'H000'
public constant ID_BM = 'H001'
public constant ID_MK = 'H002'
public constant ID_PL = 'H003'

@compiletime function createTc()
    new UnitDefinition(ID_TC, 'Otch')

@compiletime function createBm()
    new UnitDefinition(ID_BM, 'Obla')

@compiletime function createMk()
    new UnitDefinition(ID_MK, 'Hmkg')

@compiletime function createPl()
